{
  "name": "svelte-bench",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@google/genai": "^1.15.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "jsdom": "^26.1.0",
    "openai": "^5.16.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start": "pnpm run-tests && pnpm build",
    "run-tests": "tsx ./index.ts",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm merge && tsx ./build-static.ts",
    "build-v2": "pnpm merge-v2 && tsx ./build-static.ts",
    "verify": "tsx ./verify.ts",
    "merge": "tsx ./merge.ts",
    "merge-v2": "tsx ./merge-v2.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@testing-library/jest-dom": "^6.8.0",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "ollama": "^0.5.17",
    "rimraf": "^6.0.1",
    "svelte": "^5.38.5",
    "tsx": "^4.20.5",
    "undici": "^7.15.0",
    "vite": "^7.1.3"
  }
}
