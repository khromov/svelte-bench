{
  "name": "svelte-bench",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@google/genai": "^1.25.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "jsdom": "^27.0.1",
    "openai": "^6.5.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start": "pnpm run-tests && pnpm build",
    "run-tests": "tsx ./index.ts",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm merge && tsx ./build-static.ts",
    "build-v1": "tsx ./merge-v1.ts && tsx ./build-static.ts",
    "verify": "tsx ./verify.ts",
    "merge": "tsx ./merge.ts",
    "merge-v1": "tsx ./merge-v1.ts",
    "open": "node -e \"require('child_process').spawn('open', ['benchmarks/benchmark-results-merged.html'], { stdio: 'inherit' })\""
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^0.0.51",
    "@ai-sdk/anthropic": "^0.0.55",
    "@ai-sdk/azure": "^0.0.42",
    "@ai-sdk/cerebras": "^0.0.6",
    "@ai-sdk/cohere": "^0.0.16",
    "@ai-sdk/deepinfra": "^0.0.11",
    "@ai-sdk/deepseek": "^0.0.9",
    "@ai-sdk/fireworks": "^0.0.18",
    "@ai-sdk/google": "^0.0.53",
    "@ai-sdk/google-vertex": "^0.0.36",
    "@ai-sdk/groq": "^0.0.19",
    "@ai-sdk/mistral": "^0.0.26",
    "@ai-sdk/perplexity": "^0.0.9",
    "@ai-sdk/togetherai": "^0.0.9",
    "@ai-sdk/xai": "^0.0.13",
    "@anthropic-ai/sdk": "^0.67.0",
    "@modelcontextprotocol/sdk": "^1.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "ai": "^4.2.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "ollama": "^0.6.0",
    "rimraf": "^6.0.1",
    "svelte": "^5.41.0",
    "tsx": "^4.20.6",
    "undici": "^7.16.0",
    "vite": "^7.1.11"
  }
}
