    "@ai-sdk/deepseek": "^1.0.24",
    "@ai-sdk/elevenlabs": "^1.0.15",
    "@ai-sdk/fal": "^1.0.18",
    "@ai-sdk/fireworks": "^1.0.24",
    "@ai-sdk/gladia": "^1.0.14",
    "@ai-sdk/google": "^2.0.24",
    "@ai-sdk/google-vertex": "^3.0.54",
    "@ai-sdk/groq": "^2.0.25",
    "@ai-sdk/hume": "^1.0.14",
    "@ai-sdk/lmnt": "^1.0.14",
    "@ai-sdk/luma": "^1.0.14",
    "@ai-sdk/mcp": "^0.0.3",
    "@ai-sdk/mistral": "^2.0.20",
    "@ai-sdk/openai": "^2.0.56",
