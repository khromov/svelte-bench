    "@ai-sdk/google-vertex": "^3.0.64",
    "@ai-sdk/groq": "^2.0.29",
    "@ai-sdk/hume": "^1.0.18",
    "@ai-sdk/lmnt": "^1.0.18",
    "@ai-sdk/luma": "^1.0.18",
    "@ai-sdk/mcp": "^0.0.8",
    "@ai-sdk/mistral": "^2.0.24",
    "@ai-sdk/openai": "^2.0.68",
