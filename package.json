{
  "name": "svelte-bench",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@google/genai": "^1.20.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "jsdom": "^27.0.0",
    "openai": "^5.21.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start": "pnpm run-tests && pnpm build",
    "run-tests": "tsx ./index.ts",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm merge && tsx ./build-static.ts",
    "build-v1": "tsx ./merge-v1.ts && tsx ./build-static.ts",
    "verify": "tsx ./verify.ts",
    "merge": "tsx ./merge.ts",
    "merge-v1": "tsx ./merge-v1.ts",
    "open": "node -e \"require('child_process').spawn('open', ['benchmarks/benchmark-results-merged.html'], { stdio: 'inherit' })\""
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@testing-library/jest-dom": "^6.8.0",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "ollama": "^0.5.18",
    "rimraf": "^6.0.1",
    "svelte": "^5.39.0",
    "tsx": "^4.20.5",
    "undici": "^7.16.0",
    "vite": "^7.1.5"
  }
}
