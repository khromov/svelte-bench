{
  "name": "svelte-bench",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@google/genai": "^1.28.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.5",
    "jsdom": "^27.1.0",
    "openai": "^6.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start": "pnpm run-tests && pnpm build",
    "run-tests": "tsx ./index.ts",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm merge && tsx ./build-static.ts",
    "build-v1": "tsx ./merge-v1.ts && tsx ./build-static.ts",
    "verify": "tsx ./verify.ts",
    "merge": "tsx ./merge.ts",
    "merge-v1": "tsx ./merge-v1.ts",
    "open": "node -e \"require('child_process').spawn('open', ['benchmarks/benchmark-results-merged.html'], { stdio: 'inherit' })\"",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@testing-library/jest-dom": "^6.9.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "ollama": "^0.6.2",
    "rimraf": "^6.1.0",
    "svelte": "^5.43.2",
    "tsx": "^4.20.6",
    "undici": "^7.16.0",
    "vite": "^7.1.12"
  }
}
