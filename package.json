{
  "name": "svelte-bench",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@google/genai": "^1.29.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.9",
    "@testing-library/user-event": "^14.6.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.5",
    "jsdom": "^27.2.0",
    "openai": "^6.9.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "vitest": "^4.0.9"
  },
  "scripts": {
    "start": "sh -c 'pnpm run-tests \"$@\" && pnpm build' --",
    "run-tests": "tsx ./index.ts",
    "inspect-mcp": "tsx ./inspect-mcp-tools.ts",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm merge && tsx ./build-static.ts",
    "build-v1": "tsx ./merge-v1.ts && tsx ./build-static.ts",
    "verify": "tsx ./verify.ts",
    "merge": "tsx ./merge.ts",
    "merge-v1": "tsx ./merge-v1.ts",
    "open": "node -e \"require('child_process').spawn('open', ['benchmarks/benchmark-results-merged.html'], { stdio: 'inherit' })\"",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.55",
